{
    "name": "dxw/2fa",
    "description": "WordPress plugin for 2 factor authentication (TOTP and SMS)",
    "type": "wordpress-plugin",
    "license": "MIT",
    "config": {
      "platform": {
        "php": "8.2"
      }
    },
    "require": {
        "christian-riesen/otp": "1.*",
        "endroid/qrcode": "1.5.0",
        "google/recaptcha": "~1.1",
        "twilio/sdk": "4.2.0",
        "dxw/iguana": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Dxw\\TwoFa\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": "vendor/bin/phar-install"
    },
    "require-dev": {
        "10up/wp_mock": "^0.3.0",
        "dxw/phar-install": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.2",
        "peridot-php/leo": "^1.6",
        "peridot-php/peridot": "^1.19",
        "peridot-php/peridot-dot-reporter": "^1.0",
        "php-mock/php-mock-mockery": "^1.3"
    }
}
